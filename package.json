{
  "name": "rolling-fields",
  "version": "0.0.1",
  "description": "A dynamic field generator for React (not dependent on any particular form library)",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "test": "mocha",
    "build": "babel lib --out-dir dist",
    "lint": "eslint --ext .jsx,.js ./lib ./test",
    "lint:fix": "eslint --ext .jsx,.js --fix ./lib ./test",
    "prepublishOnly": "npm run build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tes/rolling-fields.git"
  },
  "keywords": [
    "react",
    "form"
  ],
  "author": "Tes Global <info@tesgobal.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tes/rolling-fields/issues"
  },
  "homepage": "https://github.com/tes/rolling-fields/#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-links": "^5.0.5",
    "@storybook/addons": "^5.0.5",
    "@storybook/react": "^5.0.11",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "env-test": "^1.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "^5.15.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "jsdom": "^14.0.0",
    "mocha": "^6.1.4",
    "react-dom": "^16.8.4",
    "sinon": "^7.2.7",
    "waait": "^1.0.4"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "prop-types": "^15.0.0",
    "react": "^16.0.0"
  }
}
